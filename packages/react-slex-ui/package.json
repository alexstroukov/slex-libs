{
  "name": "react-slex-ui",
  "version": "0.1.0",
  "description": "react-slex-ui is a component library for react ",
  "main": "compiled/index.js",
  "module": "compiled/reactSlexUi.js",
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "class-property"
    ],
    "ignore": [
      "node_modules/**"
    ],
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "fetch",
      "alert",
      "localStorage",
      "btoa",
      "atob"
    ]
  },
  "scripts": {
    "mocha": "mocha",
    "watch": "webpack --config webpack/webpack.config.js --watch",
    "build-bundle": "webpack --config webpack/webpack.config.js",
    "build-modules": "babel ./src --out-dir ./compiled && npm run copy-files",
    "build": "npm run build-bundle && npm run build-modules && npm run copy-files",
    "copy-files": "node ./scripts/copy-files.js",
    "test": "mocha compiled/test",
    "clean": "rm -rf compiled",
    "deep-clean": "rm -rf node_modules && npm run clean",
    "prepublishOnly": "rm -rf compiled && npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "release": "npm publish compiled"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.5",
    "lodash": "4.17.2",
    "material-ui": "1.0.0-beta.27",
    "material-ui-icons": "1.0.0-beta.10",
    "prop-types": "15.5.10",
    "rc-queue-anim": "1.2.2",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-motion": "0.5.1",
    "recompose": "0.26.0",
    "stackblur-canvas": "1.4.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.14",
    "@storybook/addon-links": "^3.3.14",
    "@storybook/addons": "^3.3.14",
    "@storybook/react": "^3.3.14",
    "@storybook/storybook-deployer": "2.3.0",
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "1.4.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-destructuring": "6.23.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "chai": "4.1.1",
    "core-js": "2.5.0",
    "css-loader": "0.28.6",
    "eslint-plugin-class-property": "1.0.6",
    "estraverse": "4.2.0",
    "inquirer": "1.1.3",
    "jsdom": "9.8.3",
    "json-loader": "0.5.7",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "mocha": "3.5.0",
    "node-sass": "4.5.3",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.6",
    "sinon": "3.2.1",
    "standard": "10.0.3",
    "style-loader": "0.18.2",
    "webpack": "3.5.5",
    "webpack-node-externals": "1.6.0"
  }
}
